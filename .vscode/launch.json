{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ToDoListReact-master/ToDoListReact-master/MyProject/bin/Debug/net8.0/MyProject.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ToDoListReact-master/ToDoListReact-master/MyProject",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "Docker .NET Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "netCore": {
                "appProject": "${workspaceFolder}/hw3/TodoApi/TodoApi.csproj"
            }
        }
    ]
}